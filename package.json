{
  "name": "make-url",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/make-url.umd.cjs",
  "module": "./dist/make-url.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/make-url.js",
      "require": "./dist/make-url.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && npm run test:run && tsc && vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "19.0.3",
    "@commitlint/config-conventional": "19.0.3",
    "@types/eslint": "8.56.5",
    "@types/node": "20.11.24",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "@vitest/coverage-v8": "1.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "typescript": "5.3.3",
    "vite": "5.1.5",
    "vite-plugin-dts": "3.7.3",
    "vitest": "1.3.1"
  }
}
